        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Configuration validator">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import java.nio.file.Files
import java.nio.file.Path
import java.nio.file.Paths

def reportFolderPath = (String) vars.get(&quot;reportFolderPath&quot;)

if (reportFolderPath == null || reportFolderPath.trim().isEmpty()) {
	log.error(&quot;reportFolderPath variable must be set as the absolute path of the folder where to store csv reports&quot;)
	SampleResult.setStopTestNow(true)
	
} else {
	def path = Paths.get(reportFolderPath);
	
     if (!Files.exists(path) || !Files.isDirectory(path)) {
        log.error(&quot;${path} does not exists or is not a directory&quot;)
        SampleResult.setStopTestNow(true)
     }
     else if (!Files.isReadable(path)) {
        log.error(&quot;Folder ${path} is not readable&quot;)
        SampleResult.setStopTestNow(true)
     }
     else {
     	log.error(&quot;Folder ${path} is valid and readable&quot;)
     }
}






	
</stringProp>
        </JSR223Sampler>
